[global_config]
  suppress_multiple_term_dialog = True
  title_receive_bg_color = "#204a87"
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.9
    cursor_color = "#aaaaaa"
    font = Source Code Pro 12
    scrollback_infinite = True
    use_system_font = False
